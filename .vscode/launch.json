    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run npm run dev",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "dev"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "type": "node-terminal",
            "name": "Test ListManager",
            "request": "launch",
            "command": "npx jest ListManager",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Coverage ListManager",
            "request": "launch",
            "command": "npx jest --coverage ListManager",
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "node-terminal",
            "name": "Run Checker server",
            "request": "launch",
            "command": "ts-node src/ItemCheckerDemo/src/ListServer.ts",
            "cwd": "${workspaceFolder}"
        },
    ]
}